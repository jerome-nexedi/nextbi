        <Cyan name="zyan"/>
        <Magenta name="magenta"/>
        <White name="weiß"/>
        <Blue name="blau"/>
        <Green name="grün"/>
        <Yellow name="gelb"/>
      </Colors>
      <DateOrder>DMY</DateOrder>
      <YearLimit>30</YearLimit>
