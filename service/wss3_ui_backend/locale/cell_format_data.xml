  </Locales>
  <Formats>
    <DefaultFormat id="3000">
      <Format locale_pattern="de" code="TT.MM.JJJJ"/>
      <Format locale_pattern="*" parse_locale="en_US" code="MM/DD/YYYY"/>
    </DefaultFormat>
    <!-- Don't change ID-s here as they are mapped to internal ids. Change only the desired format-->
    <DefaultFormat id="3"><!-- format for numbers -->
      <Format locale_pattern="*" parse_locale="en_US" code="#,##0.00"/>
    </DefaultFormat>
    <DefaultFormat id="4"> <!-- format for date -->
      <Format locale_pattern="*" parse_locale="en_US" code="DD/MM/YYYY"/>
    </DefaultFormat>
    <DefaultFormat id="5"> <!-- format for date time -->
      <Format locale_pattern="*" parse_locale="en_US" code="DD/MM/YYYY hh:mm"/>
    </DefaultFormat> 
  </Formats>
