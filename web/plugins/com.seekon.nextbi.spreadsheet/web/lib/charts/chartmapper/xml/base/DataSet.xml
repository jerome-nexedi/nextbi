    <arg name="fillColor" default="-1">
      <map path=""/>
    </arg>
<!--The edge color used to draw the edge of the symbol. -1 means the edge color of the data set will be used.-->
    <arg name="edgeColor" default="-1">
      <map path=""/>
    </arg>
  </args>
</setDataSymbol4>

<!--setLineWidth - Sets the line width of lines when drawing the data set.-->
<setLineWidth applicable="false" type="method" skip="true">
  <args>
<!--The width of the line in pixels.-->
    <arg name="w" mandatory="true">
      <map path=""/>
    </arg>
  </args>
</setLineWidth>

<!--setUseYAxis - Determine the y-axis to use when drawing the data set.-->
<setUseYAxis applicable="false" applicable="false" type="method" skip="true">
  <args>
<!--The y-axis to use when drawing the data set.-->
    <arg name="a" mandatory="true">
      <map path=""/>
    </arg>
  </args>
</setUseYAxis>

<!--setUseYAxis2 - Determine if the primary or secondary y-axis should be used when drawing the data set.-->
<setUseYAxis2 applicable="false" type="method" skip="true">
  <args>
<!--A true (non-zero) value means the secondary y-axis will be used. A false (zero) value means the primary y-axis will be used.-->
    <arg name="b" default="true">
      <map path=""/>
    </arg>
  </args>
</setUseYAxis2>
